{
    "name": "balsama/team_do_stats",
    "description": "Gathers stats from D.O for reporting",
    "type": "project",
    "bin": ["bin/report"],
    "require": {
        "balsama/fetch": "^0",
        "guzzlehttp/guzzle": "^7",
        "mathieuviossat/arraytotexttable": "~1.0.0",
        "gitonomy/gitlib": "^1.0",
        "symfony/filesystem": "^5",
        "symfony/process": "^5",
        "symfony/console": "^5",
        "symfony/yaml": "^5",
        "php": "^7.4"
    },
    "license": "GPL-2.0-or-later",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Balsama\\DoStats\\": "src/DoStats",
            "Balsama\\Command\\": "src/Command"
        }
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.0@dev",
        "phpunit/phpunit": "^9.3"
    }
}
